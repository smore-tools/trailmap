{
  "name": "@smore/trailmap",
  "version": "0.1.1",
  "description": "Simple component that generates @stencil/router routes, mapping a url to a content path",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rm -rf dist/ && mkdir dist/",
    "prebuild": "npm run clean",
    "build": "tsc -p .",
    "test": "jest",
    "test.watch": "jest --watch"
  },
  "peerDependencies": {
    "@stencil/core": "^0.13.1",
    "@stencil/router": "^0.3.0"
  },
  "devDependencies": {
    "@stencil/core": "^0.13.1",
    "@types/jest": "^23.3.1",
    "jest": "^23.4.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smore-tools/trailmap.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smore-tools/trailmap"
  },
  "homepage": "https://github.com/smore-tools/trailmap",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
